<script type="application/javascript">
    var responseEditor = ace.edit("response-editor");
    responseEditor.setTheme("ace/theme/xcode");
    responseEditor.getSession().setMode("ace/mode/json");
    responseEditor.setReadOnly(true);
    responseEditor.resize();
    responseEditor.setShowPrintMargin(false);
    jQuery(function ($) {
        $(".regress-btn").on('click', function () {
            var url = $(this).attr('data-url')
            showLoading(10)
            $.ajax({
                type: 'GET',
                url: "//" + host + url,
                data:{
                },
                success: function (res) {
                    responseEditor.setValue(JSON.stringify(res ,null, 2));
                    hideLoading(10)
                },
                dataType: 'json',
                error: function (XMLHttpRequest) {
                    hideLoading(10)
                    responseEditor.setValue(JSON.stringify(XMLHttpRequest, null, 2));
                }
            });
        })
    })
</script>
